        <activity
            android:name="plugin.sprd.supportcmcc.WifiTrustedAPList"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/wifi_trusted_ap_category"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.Material.Light.Dialog" >
        </activity>

        <receiver android:name="plugin.sprd.supportcmcc.WifiConnectionPolicy" >
            <intent-filter>
                <action android:name="android.net.wifi.WIFI_STATE_CHANGED" />
                <action android:name="android.net.wifi.supplicant.STATE_CHANGE" />
                <action android:name="android.net.wifi.SCAN_RESULTS" />
                <action android:name="android.net.wifi.STATE_CHANGE" />
                <action android:name="android.net.wifi.RSSI_CHANGED" />
                <action android:name="sprd.net.wifi.WIFI_CONNECTED_AP_ABSENT" />
                <action android:name="sprd.net.wifi.WIFI_DISABLED_WHEN_CONNECTED" />
                <action android:name="sprd.net.wifi.BSS_REMOVED_ACTION" />
                <action android:name="sprd.wifi.alarm.CONNECT_WIFI" />
                <action android:name="sprd.wifi.alarm.DISCONNECT_WIFI" />
                <action android:name="sprd.net.wifi.SCAN_RESULTS_AVAILABLE" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity
            android:name="plugin.sprd.supportcmcc.WifiConnectionPolicyDialogActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTask"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Material.Light.Dialog" >
        </activity>

        <activity android:name="plugin.sprd.supportcmcc.AirplaneModeWifiAlertActivity"
            android:theme="@*android:style/Theme.Holo.Dialog.Alert"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:noHistory="true"
            android:excludeFromRecents="true"
            android:launchMode="singleTop" >
        </activity>
