    <!-- support functions list start -->
    <string name="support_find_phone">Find the phone</string>
    <string name="support_find_phone_summary">Shout out loudly：\' [The wake up words] + Where are you?\', the phone will respond to you by playing the custom ring tones.</string>
    <string name="support_call_phone">Make phone calls</string>
    <string name="support_call_phone_summary">Say to the phone：\' [The wake up words] + Call Xiao Wang\'，it will call someone.</string>
    <string name="support_send_mms">Send messages</string>
    <string name="support_send_mms_summary">Say to the phone：\' [The wake up words] + Send a text message to Xiao Wang\'，it will open the messaging apps to send a message.</string>
    <string name="support_camera">Take pictures</string>
    <string name="support_camera_summary">Say to the phone：\' [The wake up words] + Take a picture for me\'，it will open the camera and take pictures directly.</string>
    <string name="support_open_music">Play Music</string>
    <string name="support_open_music_summary">Say to the phone:\' [The wake up words] + Play Wang Fei\'s \"Hong Dou\"\'，it will open the music player and play the song.</string>
    <string name="support_open_app">Launch applications</string>
    <string name="support_open_app_summary">Say to the phone：\' [The wake up words] + Open WeChat\'，the phones will launch the WeChat app.</string>
    <!-- dialog message -->
    <string name="find_phone_content">You can ask me：\n\n Where are you\n Do you hear me\n Give me a response</string>
    <string name="call_phone_content">You can ask me：\n\n Call Xiao Wang\n Call Xiaomin back\n Call customer service of China Mobile\n Dial 114</string>
    <string name="send_mms_content">You can ask me：\n\n Send Xiao Wang a text message\n Send a text message to Xiaomin\n Reply a text message to mom\n Send the meeting time to Xiaoming by SMS</string>
    <string name="open_camera_content">You can ask me：\n\n Take a picture for me\n Take a picture of the scenery\n Take a picture of the signpost.</string>
    <string name="open_music_content">You can ask me：\n\n Play Wang Fei\'s \"Hong Dou\"\n Play a song at random\n I want to listen to "Pao Mo"\n Play a song by Zhang Xueyou</string>
    <string name="open_app_content">You can ask me：\n\n Open WeChat\n Download a "compass" app\n Launch the notepad</string>
    <!-- support functions list end -->

