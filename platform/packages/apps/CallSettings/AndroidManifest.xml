<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.callsettings"
    android:sharedUserId="android.uid.phone" >

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:label="@string/phoneAppLabel"
        android:persistent="true"
        android:process="com.android.phone"
        android:icon="@mipmap/ic_launcher_phone"
        android:supportsRtl="true" >
        <activity
            android:name="com.android.callsettings.callforward.GsmUmtsAllCallForwardOptions"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/labelCF"
            android:theme="@style/SettingsLight"
            android:excludeFromRecents="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.callsettings.callforward.GsmUmtsCallForwardOptions"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/labelCF"
            android:theme="@style/SettingsLight"
            android:excludeFromRecents="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.callsettings.callforward.GsmUmtsVideoCallForwardOptions"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/labelCF"
            android:theme="@style/SettingsLight"
            android:excludeFromRecents="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.callsettings.callforward.CallForwardTimeEditPreference"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/labelCFT"
            android:theme="@style/SettingsLight"
            android:excludeFromRecents="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <!-- SPRD: LND/SDN FEATURE @{ -->
        <activity
            android:name="com.android.callsettings.lndsdn.LndOrSdnListActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenSize"
            android:label="@string/sdn_list"
            android:theme="@style/DialerSettingsLight"
            android:clearTaskOnLaunch="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <!-- @} -->
        <!-- SPRD: FEATURE_VOLTE_LOCK @{ -->
        <activity
            android:name="com.android.callsettings.VoLTEConfigSettings"
            android:configChanges="keyboardHidden"
            android:label="@string/volte_lock"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <!-- @} -->

        <!--  SPRD:FEATURE_FDN @{ -->
        <activity android:name="com.android.callsettings.fdnsettings.FdnList"
                android:label="@string/fdnListLabel"
                android:theme="@style/FdnListDialerSettingsLight">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="vnd.android.cursor.item/sim-contact" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.callsettings.fdnsettings.FdnSetting"
                android:configChanges="keyboardHidden|orientation|screenSize"
                android:label="@string/fdn"
                android:theme="@style/DialerSettingsLight"
                android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.callsettings.fdnsettings.GetPin2Screen"
                android:configChanges="keyboardHidden|orientation|screenSize"
                android:label="@string/get_pin2"
                android:theme="@style/DialerSettingsLight"
                android:windowSoftInputMode="stateVisible">
        </activity>

        <activity android:name="com.android.callsettings.fdnsettings.EditFdnContactScreen"
                android:theme="@style/DialerSettingsLight"
                android:windowSoftInputMode="stateVisible">
        </activity>

        <activity android:name="com.android.callsettings.fdnsettings.DeleteFdnContactScreen"
                android:theme="@style/DialerSettingsLight"
                android:label="@string/delete_fdn_contact">
        </activity>
        <!-- @} -->
        <activity
            android:name="com.android.callsettings.callbarring.CallBarringOptions"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/labelCF"
            android:theme="@style/SettingsLight" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.callsettings.callbarring.CallInBarringEditPreference"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/labelCIN"
            android:theme="@style/SettingsLight" >
        </activity>
        <activity
            android:name="com.android.callsettings.callbarring.CallOutBarringEditPreference"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/labelCOUT"
            android:theme="@style/SettingsLight" >
        </activity>
        <!-- SPRD: FAST DIAL FEATURE -->
        <activity
            android:name="com.android.callsettings.fastdial.FastDialSettingActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/fast_dial_settings"
            android:theme="@style/DialerSettingsLight"
            android:process="com.android.dialer">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.FASTDIAL" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <receiver android:name=".fastdial.FastDialReceiver"
            android:process="com.android.dialer" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action android:name="android.intent.action.SIM_STATE_CHANGED" />
                <action android:name="android.intent.action.LOCALE_CHANGED" />
            </intent-filter>
        </receiver>
        <!-- END -->
    </application>

</manifest>
